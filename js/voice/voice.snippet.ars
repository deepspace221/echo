                        result = "‚ö† " + logDate+ "-" +logTime + ": The designeted **1 hour** buffer (" + timeCooldown +") from last ping has not been reached. Ping for " + vcUserName  +" at **" + vcName + "** has been aborted.";
                    else if (type == "public")
                        result = "‚ö† " + logDate+ "-" + logTime + ": " + vcUserName + " has **" + status + "** voice channel **" + vcName + "**. No pinging is scheduled for this channel";
        }       
        return result; 
}}
